<script>
  function countall(){
    //统计指标-全部
    $("#kiplist option").attr("selected", "selected");
    $("#kiplist").change();
    $(".countall").replaceWith('<a  style="cursor: hand;"  class="discountall" onclick="discountall();">(取消全选)</a>');
  }
    
  function discountall(){
    //统计指标-取消全部
    $("#kiplist option").removeAttr("selected");
    $("#kiplist").change();
    $(".discountall").replaceWith('<a  style="cursor: hand;" onclick="countall();"  class="countall">(全选)</a>');
  }
</script>
